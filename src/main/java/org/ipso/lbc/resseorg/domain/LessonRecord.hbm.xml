<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.ipso.lbc.resseorg.domain.LessonRecord" table="LESSON_RECORDS">
        <composite-id>
            <key-property name="studentId" column="序号"/>
            <key-property name="name" column="姓名"/>
        </composite-id>
        <property name="minutesToAdd" column="应加上课时间"/>
        <property name="lateTimesToSubtract" column="应减迟到次数"/>
        <property name="info" column="备注"/>
        <property name="updateTime" column="更新时间"/>

    </class>
</hibernate-mapping>
